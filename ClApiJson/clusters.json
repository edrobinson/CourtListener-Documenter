            "procedural_history": {
                "type": "string",
                "required": false,
                "read_only": false,
                "label": "Procedural history",
                "help_text": "The history of the case as it jumped from court to court"
            },
            "attorneys": {
                "type": "string",
                "required": false,
                "read_only": false,
                "label": "Attorneys",
                "help_text": "The attorneys that argued the case, as free text"
            },
            "nature_of_suit": {
                "type": "string",
                "required": false,
                "read_only": false,
                "label": "Nature of suit",
                "help_text": "The nature of the suit. For the moment can be codes or laws or whatever"
            },
            "posture": {
                "type": "string",
                "required": false,
                "read_only": false,
                "label": "Posture",
                "help_text": "The procedural posture of the case."
            },
            "syllabus": {
                "type": "string",
                "required": false,
                "read_only": false,
                "label": "Syllabus",
                "help_text": "A summary of the issues presented in the case and the outcome."
            },
            "headnotes": {
                "type": "string",
                "required": false,
                "read_only": false,
                "label": "Headnotes",
                "help_text": "Headnotes are summary descriptions of the legal issues discussed by the court in the particular case. They appear at the beginning of each case just after the summary and disposition. They are short paragraphs with a heading in bold face type. From Wikipedia - A headnote is a brief summary of a particular point of law that is added to the text of a courtdecision to aid readers in locating discussion of a legalissue in an opinion. As the term implies, headnotes appearat the beginning of the published opinion. Frequently, headnotes are value-added components appended to decisions by the publisher who compiles the decisions of a court for resale. As handed down by the court, a decision or written opinion does not contain headnotes. These are added later by an editor not connected to the court, but who instead works for a legal publishing house."
            },
            "summary": {
                "type": "string",
                "required": false,
                "read_only": false,
                "label": "Summary",
                "help_text": "A summary of what happened in the case. Appears at the beginning of the case just after the title of the case and court information."
            },
            "disposition": {
                "type": "string",
                "required": false,
                "read_only": false,
                "label": "Disposition",
                "help_text": "Description of the procedural outcome of the case, e.g. Reversed, dismissed etc. Generally a short paragraph that appears just after the summary or synopsis"
            },
            "history": {
                "type": "string",
                "required": false,
                "read_only": false,
                "label": "History",
                "help_text": "History of the case (similar to the summary, but focused on past events related to this case). Appears at the beginning of the case just after the title of the case and court information"
            },
            "other_dates": {
                "type": "string",
                "required": false,
                "read_only": false,
                "label": "Other dates",
                "help_text": "Other date(s) as specified in the text (case header). This may include follow-up dates."
            },
            "cross_reference": {
                "type": "string",
                "required": false,
                "read_only": false,
                "label": "Cross reference",
                "help_text": "Cross-reference citation (often to a past or future similar case). It does NOT identify this case."
            },
            "correction": {
                "type": "string",
                "required": false,
                "read_only": false,
                "label": "Correction",
                "help_text": "Publisher's correction to the case text. Example: Replace last paragraph on page 476 with this text: blah blah blah. This is basically an unstructured text that can be used to manually correct case content according to publisher's instructions. No footnotes is expected within it."
            },
            "citation_count": {
                "type": "integer",
                "required": false,
                "read_only": false,
                "label": "Citation count",
                "help_text": "The number of times this document is cited by other opinion",
                "min_value": -2147483648,
                "max_value": 2147483647
            },
            "precedential_status": {
                "type": "choice",
                "required": false,
                "read_only": false,
                "label": "Precedential status",
                "help_text": "The precedential status of document, one of: Published, Unpublished, Errata, Separate, In-chambers, Relating-to, Unknown",
                "choices": [
                    {
                        "value": "Published",
                        "display_name": "Precedential"
                    },
                    {
                        "value": "Unpublished",
                        "display_name": "Non-Precedential"
                    },
                    {
                        "value": "Errata",
                        "display_name": "Errata"
                    },
                    {
                        "value": "Separate",
                        "display_name": "Separate Opinion"
                    },
                    {
                        "value": "In-chambers",
                        "display_name": "In-chambers"
                    },
                    {
                        "value": "Relating-to",
                        "display_name": "Relating-to orders"
                    },
                    {
                        "value": "Unknown",
                        "display_name": "Unknown Status"
                    }
                ]
            },
            "date_blocked": {
