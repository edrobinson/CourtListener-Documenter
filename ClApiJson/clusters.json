                                },
                                {
                                    "value": 5,
                                    "display_name": "A citation in an early SCOTUS reporter (e.g. 5 Black. 55)"
                                },
                                {
                                    "value": 6,
                                    "display_name": "A citation in the Lexis system (e.g. 5 LEXIS 55)"
                                },
                                {
                                    "value": 7,
                                    "display_name": "A citation in the WestLaw system (e.g. 5 WL 55)"
                                },
                                {
                                    "value": 8,
                                    "display_name": "A vendor neutral citation (e.g. 2013 FL 1)"
                                },
                                {
                                    "value": 9,
                                    "display_name": "A law journal citation within a scholarly or professional legal periodical (e.g. 95 Yale L.J. 5; 72 Soc.Sec.Rep.Serv. 318)"
